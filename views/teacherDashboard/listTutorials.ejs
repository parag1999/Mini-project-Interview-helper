<% include ../partials/teacherHeader.ejs %>


    <% tutorials.forEach(tutorial => { %>
        <div class="card">
            <div class="card-header">
                <%= tutorial.subject %>
            </div>
            <div class="card-body">
                <h5 class="card-title">Link : </h5>
                <p class="card-text">
                    <%= tutorial.link %>
                </p>
                <a href="/deleteTutorial/<%= tutorial.tutorial_id %>" class="btn btn-primary">Delete</a>
                <a href="/editTutorial/<%= tutorial.tutorial_id %>" class="btn btn-primary">Update</a>
            </div>
        </div>
        <br>
        <br>
        <% }) %>