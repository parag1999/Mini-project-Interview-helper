<% include ../partials/studentHeader.ejs %>

<div class="jumbotron" style="background:transparent !important">
        <h1 class="display-4">Hello, <%= student.fname %></h1>
    <div class="card-deck">
        <% allNotes.forEach(note => { %>
        <div class="card">
            <div class="card-header">
                <%= note.note_name %>
            </div>
            <div class="card-body">
                <h5 class="card-title">Subject : <%= note.subject %></h5>
                <p class="card-text">
                    Total Time: <%= note.total_time %>
                </p>
                <a href="/notesQuestions/<%= note.note_id %>" class="btn btn-primary">View Notes</a>
            </div>
        </div>
        <% }) %>
    </div>
                        
</div>